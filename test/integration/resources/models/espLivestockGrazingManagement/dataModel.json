{
    "dataModel": [
        {
            "dataType": "text",
            "name": "multiplePaddocks",
            "constraints": [
                "Yes",
                "No"
            ]
        },
        {
            "columns": [
                {
                    "dataType": "number",
                    "name": "paddockNum",
                    "behaviour": [{
                        "condition": "multiplePaddocks == \"Yes\"",
                        "type": "enable"
                    }]
                },
                {
                    "dataType": "date",
                    "name": "startDate",
                    "description": "",
                    "validate": "required"
                },
                {
                    "dataType": "date",
                    "name": "endDate",
                    "description": "",
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "typeOfStock",
                    "description": "",
                    "constraints": [
                        "Cattle",
                        "Sheep",
                        "Goats",
                        "Horses"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "productionType",
                    "description": "",
                    "constraints": {
                        "default": [],
                        "options": [
                            {
                                "condition": "typeOfStock == \"Cattle\"",
                                "value": [
                                    "N/A",
                                    "Dairy",
                                    "Meat"
                                ]
                            },
                            {
                                "condition": "typeOfStock == \"Sheep\"",
                                "value": [
                                    "N/A",
                                    "Dairy",
                                    "Meat",
                                    "Wool"
                                ]
                            },
                            {
                                "condition": "typeOfStock == \"Goats\"",
                                "value": [
                                    "N/A",
                                    "Dairy",
                                    "Meat",
                                    "Fibre"
                                ]
                            },
                            {
                                "condition": "typeOfStock == \"Horses\"",
                                "value": [
                                    "N/A",
                                    "Adjistment"
                                ]
                            }
                        ],
                        "type": "computed"
                    }
                },
                {
                    "dataType": "text",
                    "name": "growthStage",
                    "description": "",
                    "constraints": {
                        "default": [],
                        "options": [
                            {
                                "condition": "typeOfStock == \"Cattle\"",
