                {
                    "type": "col",
                    "items": [
                        {
                            "preLabel": "History - Does the tree any have historic association?",
                            "source": "historicValue",
                            "type": "selectOne"
                        },
                        {
                            "preLabel": "Form - Does the tree have an outstanding or abnormal growth form?",
                            "source": "growthFormCharacter",
