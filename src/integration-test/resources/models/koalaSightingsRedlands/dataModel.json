{
    "modelName": "koalaSightingsRedlands",
    "record": "true",
    "dataModel": [
        {
            "dataType": "date",
            "name": "eventDate",
            "dwcAttribute": "eventDate",
            "description": "The date on which the sighting was made.",
            "validate": "required"
        },
        {
            "dataType": "time",
            "name": "eventStartTime",
            "dwcAttribute": "eventTime",
            "description": "The time at which the sighting was made.",
            "validate": "required"
        },
        {
            "dataType": "text",
            "name": "recordedBy",
            "dwcAttribute": "recordedBy",
            "description": "The name of the person who is attributed with making the sighting.",
            "noEdit": "true",
            "validate": "required"
        },
        {
            "dataType": "image",
            "name": "sightingPhoto",
            "description": "Upload a photo taken of the koala at the time of your sighting. This is essential for verification of the record."
        },
        {
            "dataType": "species",
            "name": "species",
            "dwcAttribute": "scientificName",
            "description": "The scientific name of the koala",
            "noEdit": "true",
            "validate": "required"
        },
        {
            "defaultValue": "1",
            "dataType": "number",
            "name": "individualCount",
            "dwcAttribute": "individualCount",
            "description": "The number of individual koalas observed at this place and time.",
            "validate": "min[1]"
        },
        {
            "dataType": "text",
            "name": "locationContext",
            "description": "This describes the type of landuse around where the koala was sighted.",
            "constraints": [
                "Roadside",
                "Residential ¼ ac (1000 m2) or less",
                "Residential over  ¼ ac  (1000 m2)",
                "Park/School or similar",
                "Commercial or Industrial",
                "Creek line or bush"
            ]
        },
        {
            "dataType": "text",
            "name": "treeSpecies",
            "description": "If the koala was sighted in a tree, please indivate the species of the tree if you know it, or describe it as best you can."
        },
        {
            "dataType": "text",
            "name": "gender",
            "description": "Please indicate the age and/or sex of the koala if you are able to identify it.",
            "constraints": [
                "Adult female",
                "Female & young",
                "Adult male",
                "Adult unknown",
                "Juvenile female",
                "Juvenile male",
                "Juvenile unknown"
            ]
        },
        {
            "dataType": "text",
            "name": "condition",
            "description": "Please describe the general condition of the koala, especially if it appeared to be sick or injured."
        },
        {
            "dataType": "text",
            "name": "sightingFrequency",
            "description": "Please indicate how often you have seen koalas at this location.",
            "constraints": [
                "Weekly",
                "Monthly",
                "Yearly",
                "Rarely (less that once a year)"
            ]
        },
        {
            "dataType": "text",
            "name": "eventRemarks",
            "dwcAttribute": "eventRemarks",
            "description": "General notes about the observation event, including any characteristic site features, conditions, etc."
        },
        {
            "dataType": "text",
            "name": "observerName",
            "description": "The name of the person who made the observation, if different from the person who made the record."
        },
        {
            "dataType": "text",
            "name": "piSaringAgreement",
            "description": "Your contact information forms part of your registration details on this website. Select 'Yes' if you consent to the Atlas of Living Australia providing your contact details to KAG.",
            "constraints": [
                "Yes",
                "No"
            ],
            "validate": "required"
        },
        {
            "dataType": "text",
            "name": "kagContactAgreement",
            "description": "A member of the Koala Action Group may need to contact you to confirm certain aspects of your sighting. Select 'Yes' if you agree to be contacted by KAG for this purpose.",
            "constraints": [
                "Yes",
                "No"
            ],
            "validate": "required"
        },
        {
            "columns": [
                {
                    "dwcAttribute": "verbatimLatitude",
                    "source": "locationLatitude"
                },
                {
                    "dwcAttribute": "verbatimLongitude",
                    "source": "locationLongitude"
                }
            ],
            "dataType": "geoMap",
            "name": "location",
            "dwcAttribute": "verbatimCoordinates",
            "validate": "required"
        },
        {
