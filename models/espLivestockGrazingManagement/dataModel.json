{
  "title": "Livestock Grazing Management",
  "modelName": "ESP Livestock Grazing Management",
  "dataModel": [
    {
      "dataType": "list",
      "name": "grazingPeriods",
      "columns": [
        {
          "dataType": "date",
          "description": "",
          "name": "startDate",
          "validate": "required"
        },
        {
          "dataType": "date",
          "description": "",
          "name": "endDate",
          "validate": "required"
        },
        {
          "dataType": "number",
          "description": "",
          "name": "swardHeight",
          "validate": "required,min[0]"
        },
        {
          "dataType": "text",
          "description": "",
          "name": "typeOfStock",
          "constraints": [
            "Cattle",
            "Sheep",
            "Goats",
            "Horses"
          ],
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "",
          "name": "productionType",
          "constraints": {
            "default": [],
            "type": "computed",
            "options": [
              {
                "condition": "typeOfStock == \"Cattle\"",
                "value": [
                  "N/A",
                  "Dairy",
                  "Meat"
                ]
              },
              {
                "condition": "typeOfStock == \"Sheep\"",
                "value": [
                  "N/A",
                  "Dairy",
                  "Meat",
                  "Wool"
                ]
              },
              {
                "condition": "typeOfStock == \"Goats\"",
                "value": [
                  "N/A",
                  "Dairy",
                  "Meat",
                  "Fibre"
                ]
              },
              {
                "condition": "typeOfStock == \"Horses\"",
                "value": [
                  "N/A",
                  "Adjistment"
                ]
              }
            ]
          },
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "",
          "name": "growthStage",
          "constraints": {
            "default": [],
            "type": "computed",
            "options": [
              {
                "condition": "typeOfStock == \"Cattle\"",
                "value": [
                  "Bull",
                  "Calf",
                  "Cow",
                  "Cow - Pregnant",
                  "Heifer",
                  "Steer",
                  "Weanling"
                ]
              },
              {
                "condition": "typeOfStock == \"Sheep\"",
                "value": [
                  "Ewe",
                  "Ewe - Pregnant",
                  "Hoggart",
                  "Lamb",
                  "Ram",
                  "Wether"
                ]
              },
              {
                "condition": "typeOfStock == \"Goats\"",
                "value": [
                  "Buck",
                  "Weaner",
                  "Doe",
                  "Doe - Pregnant",
                  "Kid"
                ]
              },
              {
                "condition": "typeOfStock == \"Horses\"",
                "value": [
                  "Colt",
                  "Filly",
                  "Foal",
                  "Gelding",
                  "Mare",
                  "Mare - Pregnant",
                  "Stallion",
                  "Yearling"
                ]
              }
            ]
          },
          "validate": "required"
        },
        {
          "dataType": "number",
          "description": "",
          "name": "individualCount",
          "validate": "required,min[0]"
        },
        {
          "dataType": "text",
          "description": "",
          "name": "breed",
          "constraints": {
            "default": [],
            "type": "computed",
            "options": [
              {
