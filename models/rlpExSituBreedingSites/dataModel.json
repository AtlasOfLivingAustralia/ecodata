{
  "title": "Establishing and maintaining breeding programs",
  "modelName": "RLP - Establishing ex-situ breeding programs",
  "dataModel": [
    {
      "dataType": "list",
      "name": "breedingProgramDetails",
      "columns": [
        {
          "dataType":"text",
          "name":"inSituExSitu",
          "validate":"required",
          "constraints":[
            "Ex-situ",
            "In-situ"
          ]
        },
        {
          "dataType":"text",
          "name":"newOrMaintained",
          "validate": "required",
          "constraints": [
            "Newly established",
            "Maintained"
          ]
        },
        {
          "dataType":"number",
          "name":"numberOfDays",
          "validate":"min[0]"
        },
        {
          "dataType":"feature",
          "name":"sitesOfBreedingProgram",
          "validate":"required"
        },
        {
          "dataType":"number",
          "name":"areaOfEnclosureHa",
          "validate":"required",
          "computed": {
            "expression": "$geom.areaHa(sitesOfBreedingProgram)",
            "dependents": {
              "source": [
                "sitesOfBreedingProgram"
              ]
            }
          }
        },
        {
          "dataType":"text",
          "name":"breedingTechnique",
          "description":"e.g. Seed bank or captive breeding",
          "validate":"required"
        },
        {
          "dataType": "species",
          "description": "The threatened species targeted by the breeding program",
          "name": "targetSpecies",
          "dwcAttribute": "scientificName",
          "validate": "required"
        },
        {
          "dataType":"number",
          "name":"numberOfIndividuals",
          "validate":"required,min[0]"
        },
        {
          "name":"numberOfIndividualsReleased",
          "dataType":"number",
          "validate":"required,min[0]"
        }
      ]
    }
  ],
  "viewModel": [
    {
      "items": [
        {
          "source": "breedingProgramDetails",
          "columns": [
            {
              "type":"selectOne",
              "source":"inSituExSitu",
              "title":"Ex-situ / In-situ",
              "width":"10%"
            },
            {
              "type":"selectOne",
              "title":"Is this a newly established or maintained breeding program?",
              "source":"newOrMaintained",
              "width":"10%"
            },
            {
              "type":"number",
              "width":"8%",
              "source":"numberOfDays",
              "title":"Number of days maintaining breeding program"
            },
            {
              "type":"feature",
              "width":"5%",
              "source":"sitesOfBreedingProgram",
              "title":"Site/s of breeding program"
            },
            {
              "type":"number",
              "readonly":"readonly",
              "width":"8%",
              "title":"Area (Ha) of feral-free enclosures established or maintained",
              "source":"areaOfEnclosureHa"
            },
            {
              "type":"text",
              "source":"breedingTechnique",
              "title":"Technique of ex situ breeding program",
              "width":"28%"
            },
            {
              "title": "Targeted threatened species",
              "source": "targetSpecies",
              "type": "speciesSelect",
              "width":"25%"
            },
            {
              "title":"Number of individuals in breeding program",
              "source":"numberOfIndividuals",
              "type":"number",
              "width":"8%"
            },
            {
              "title":"Number of individuals released or established in the wild",
              "source":"numberOfIndividualsReleased",
              "type":"number",
              "width":"8%"
            },

          ],
          "userAddedRows": true,
          "type": "table"
        }
      ],
      "type": "section"
    }
  ]
}