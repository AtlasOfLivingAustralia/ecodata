    {
      "name": "Emergency interventions to mitigate risks to threatened species",
      "optionalQuestionText": null,
      "optional": true,
      "collapsedByDefault": false,
      "templateName": "emergencyInterventionsToMitigateRisksToThreatenedSpecies",
      "template": {
        "modelName": "Emergency interventions to mitigate risks to threatened species",
        "dataModel": [
          {
            "dataType": "list",
            "name": "interventionDetails",
            "columns": [
              {
                "name": "interventionActivity",
                "dataType": "text",
                "validate": "required,maxSize[1000]",
                "description": "Enter emergency intervention type e.g. captive breeding, temporary holding, predator control, habitat"
              },
              {
                "name": "predatorSpecies",
                "dataType": "species",
                "description": "Enter scientific name of predator species"
              },
              {
                "name": "numberControlled",
                "dataType": "number",
                "validate": "min[0]",
                "description": "Enter estimated number of individuals control"
              },
              {
                "name": "supportedSpecies",
                "dataType": "species",
                "validate": "required",
                "description": "Enter scientific name of target species supported or protected by intervention activity"
              },
              {
                "name": "numberSupported",
                "dataType": "number",
                "validate": "required,min[0]",
                "description": "Enter estimated number of individuals salvaged or supported by intervention activity"
              },
              {
                "name": "locationOfActivity",
                "dataType": "text",
                "validate": "required,maxSize[1000]",
                "description": "Enter location(s) of where the intervention activity was  undertaken e.g. postcode, area or upload site"
              },
              {
                "name": "comments",
                "dataType": "text",
                "validate": "maxSize[1000]"
              }
            ]
          },
          {
            "name": "predatorSpeciesCount",
            "type": "number",
            "decimalPlaces": 0,
            "computed": {
              "expression": "count(interventionDetails, \"predatorSpecies\")"
            }
          },
          {
            "name": "totalNumberControlled",
            "type": "number",
            "decimalPlaces": 0,
            "computed": {
              "expression": "sum(interventionDetails, \"numberControlled\")"
            }
          },
          {
            "name": "targetSpeciesCount",
            "type": "number",
            "decimalPlaces": 0,
            "computed": {
              "expression": "count(interventionDetails, \"supportedSpecies\")"
            }
          },
          {
            "name": "totalNumberSupported",
            "type": "number",
            "decimalPlaces": 0,
            "computed": {
              "expression": "sum(interventionDetails, \"numberSupported\")"
            }
          }
        ],
        "viewModel": [
          {
            "type": "table",
            "source": "interventionDetails",
            "userAddedRows": true,
            "columns": [
              {
                "type": "text",
                "source": "interventionActivity",
                "width": "17%",
                "title": "Intervention activity"
              },
              {
                "type": "speciesSelect",
                "source": "predatorSpecies",
                "width": "18%",
                "title": "Predator Species. NOTE: only applicable where predator control has been implemented"
              },
              {
                "type": "number",
                "source": "numberControlled",
