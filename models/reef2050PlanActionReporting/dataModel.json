{
  "title": "Reef 2050 Plan Action Reporting",
  "modelName": "Reef 2050 Plan Action Reporting",
  "dataModel": [
    {
      "dataType": "list",
      "name": "agencyContacts",
      "columns": [
        {
          "dataType": "text",
          "name": "agencyContact",
          "validate": "max[100]"
        }
      ]
    },
    {
      "dataType": "list",
      "name": "actions",
      "columns": [
        {
          "dataType": "text",
          "description": "",
          "name": "actionId",
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "",
          "name": "actionDescription",
          "validate": "required"
        },
        {
          "dataType": "text",
          "name": "theme",
          "constraints": [
            "Water quality",
            "Biodiversity",
            "Ecosystem health",
            "Heritage",
            "Community benefits",
            "Economic benefits",
            "Governance"
          ],
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "Select the most appropriate progress against an action from the drop-down list",
          "name": "status",
          "constraints": [
            "Completed",
            "In place",
            "On track / Underway",
            "Delayed or limited progress",
            "Significant delays or no progress",
            "Not yet due"
          ],
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "Provide a 2-3 sentence description of the action –include detail on what the action entails and key performance indicators for what success looks like",
          "name": "description",
          "validate": "required,max[1500]"
        },
        {
          "dataType": "text",
          "description": "Provide 2-3 sentences outlining progress in implementing the action",
          "name": "progress",
          "validate": "required,max[1500]"
        },
        {
          "dataType": "stringList",
          "description": "Select organisations that are contributing to implementing the action",
          "name": "deliveryPartners",
          "constraints": [
            "NPSR",
            "DNRM",
            "DATSIP",
            "EHP",
            "DOEE",
            "DAF",
            "DTMR",
            "AMSA",
            "GBRMPA",
            "DSITI",
            "DILGP",
            "TMESBCG",
            "OGC",
            "DSD",
            "DIRD",
            "QPS",
            "DAFF",
            "DPC",
            "Ports Australia",
            "Queensland Ports Association",
            "Traditional Owners",
            "Local Government",
            "DPM&C",
            "ACIC",
            "James Cook University",
            "BHP Billiton",
            "DoEE – Reef Trust",
            "CSIRO",
            "LGAQ",
            "AMPTO",
            "Q-CAS partners",
            "GBRF",
            "AIMS",
            "CQU",
            "UQ",
            "RRRC",
            "GU",
            "NQBP",
            "River Improvement Trusts",
            "RWQPP Partnership Committee",
            "CANEGROWERS",
            "FBA",
            "NQ Dry Tropics",
            "Agforce"
          ]
        },
        {
          "dataType": "date",
          "behaviour": [
            {
              "condition": "status != \"Completed\" and status != \"In place\"",
              "type": "enable"
            }
          ],
          "description": "Provide an indicative timeframe for when the action will be completed",
          "name": "timeframe",
          "validate": "required"
        },
        {
          "dataType": "text",
          "description": "Provide an overview of milestones and deliverables for implementing the action",
          "name": "tasksAndMilestones",
          "validate": "required,max[2000]"
        },
        {
          "dataType": "number",
          "description": "Where possible, provide an estimate of funds expended to date on this action",
          "name": "fundsExtendedToDate",
          "validate": "integer"
        },
        {
          "dataType": "stringList",
          "description": "Please supply a comma separated list of URLs providing information about this action",
          "name": "webLinks",
          "validate": "required,custom[url]"
        },
        {
          "dataType": "stringList",
          "name": "linkedOrRelatedActions",
          "constraints": []
        }
      ]
    }
  ],
  "viewModel": [
    {
      "items": [
        {
          "items": [
            {
              "source": "agencyContacts",
              "columns": [
                {
                  "title": "Agency Contact(s)",
                  "source": "agencyContact",
                  "width": "100%",
                  "class": "input-xxlarge",
                  "type": "text"
                }
              ],
              "userAddedRows": true,
              "type": "table"
            }
          ],
          "type": "col"
        }
      ],
      "type": "row"
    },
    {
      "items": [
        {
          "source": "<b>Please complete a row for each action you are required to report on.</b>",
          "type": "literal"
        }
      ],
      "type": "row"
    },
    {
      "items": [
        {
          "source": "actions",
          "columns": [
            {
              "title": "Action ID",
              "readOnly": true,
              "source": "actionId",
              "width": "4%",
              "type": "text"
            },
            {
              "title": "Action",
              "readOnly": true,
              "source": "actionDescription",
              "width": "11%",
              "type": "text"
            },
            {
              "title": "Theme",
              "readOnly": true,
              "source": "theme",
              "width": "6%",
              "type": "text"
            },
            {
              "title": "Status",
              "source": "status",
              "width": "6%",
              "type": "selectOne"
            },
            {
              "title": "Description",
              "source": "description",
              "width": "11%",
              "type": "textarea",
              "rows":10
            },
            {
              "title": "Progress",
              "source": "progress",
              "width": "11%",
              "type": "textarea",
              "rows":10
            },
            {
              "title": "Delivery Partners",
              "source": "deliveryPartners",
              "width": "6%",
              "type": "select2Many"
            },
            {
              "title": "Timeframe",
              "source": "timeframe",
              "width": "6%",
              "type": "date",
              "displayOptions": {
                "minViewMode": "months"
              }
            },
            {
              "title": "Tasks and milestones / deliverables",
              "source": "tasksAndMilestones",
              "width": "11%",
              "type": "textarea",
              "rows":10
            },
            {
              "title": "Funds expended to date for action delivery",
              "source": "fundsExtendedToDate",
              "width": "8%",
              "type": "currency"
            },
            {
              "title": "Web links",
              "source": "webLinks",
              "width": "11%",
              "type": "multiInput"
            },
            {
              "title": "Linked or related actions",
              "readOnly": true,
              "source": "linkedOrRelatedActions",
              "width": "6%",
              "type": "selectMany"
            }
          ],
          "userAddedRows": false,
          "type": "table"
        }
      ],
      "type": "row"
    }
  ]
}