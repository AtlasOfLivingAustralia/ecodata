{
    "modelName": "saWeedAbundanceAndThreat",
    "dataModel": [
        {
            "dataType": "list",
            "name": "weedAbundanceAndThreatScore",
            "todo": "//move to view model",
            "columns": [
                {
                    "dataType": "species",
                    "name": "species"
                },
                {
                    "dataType": "boolean",
                    "name": "isUnknownIfWeed"
                },
                {
                    "dataType": "number",
                    "name": "areaCoveredPercent",
                    "validate": "min[0],max[100]"
                },
                {
                    "dataType": "number",
                    "name": "coverRating",
                    "constraints": [
                        "1",
                        "2",
                        "3",
                        "4",
                        "5",
                        "6"
                    ]
                },
                {
                    "dataType": "number",
                    "name": "invasiveThreatCategory",
                    "constraints": [
                        "1",
                        "2",
                        "3",
                        "4",
                        "5"
                    ]
                },
                {
                    "dataType": "number",
                    "name": "abundanceAndThreatScore",
                    "computed": {
                        "operation": "times",
                        "dependents": [
                            "coverRating",
                            "invasiveThreatCategory"
                        ]
                    }
                }
            ],
            "userAddedRows": "true"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "totalCoverPercent",
            "validate": "min[0],max[100]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "totalAbundanceAndThreatScore",
            "computed": {
                "operation": "sum",
                "dependents": {
                    "source": "abundanceAndThreatScore",
                    "fromList": "weedAbundanceAndThreatScore"
                }
            }
        }
    ],
    "viewModel": [
