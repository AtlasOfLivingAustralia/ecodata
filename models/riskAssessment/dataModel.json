{
    "modelName": "Threatening Processes & Site Condition Risks",
    "dataModel": [{
        "dataType": "list",
        "name": "riskTable",
        "columns": [
            {
                "dataType": "text",
                "name": "riskType",
                "constraints": [
                    "Cropping & root zone disturbance",
                    "Defoliation - birds",
                    "Defoliation - insects",
                    "Defoliation - other",
                    "Drainage - prolonged soil profile drying",
                    "Fire - managed fire frequency",
                    "Fire - managed fire intensity",
                    "Fire - wildfire",
                    "Flood - frequency",
                    "Flood - intensity",
                    "Flood - prolonged inundation",
                    "Grazing pressure - domestic stock",
                    "Grazing pressure - feral animals",
                    "Grazing pressure - native wildlife",
                    "Mistletoe or other parasite/fungal infestations",
                    "Soil acidification or acid-sulphate soils",
                    "Soil compaction - animal/human/machinery",
                    "Soil nutrient enrichment/eutrophication",
                    "Soil salinity",
                    "Weedicides/pesticides or chemical contamination",
                    "-- Other (specify in notes) --"
                ]
            },
            {
                "dataType": "text",
                "name": "riskDescription"
            },
            {
                "dataType": "text",
                "name": "riskLikelihood",
                "constraints": [
                    "Almost certain",
                    "Likely",
                    "Likely",
                    "Possible",
                    "Unlikely",
                    "Remote"
                ]
            },
            {
                "dataType": "text",
                "name": "riskConsequence",
                "constraints": [
                    "Insignificant",
                    "Minor",
                    "Moderate",
                    "Major",
                    "Extreme"
                ]
            },
            {
                "dataType": "text",
                "name": "riskRating",
                "constraints": [
                    "High",
                    "Significant",
                    "Medium",
                    "Low"
                ]
            }
        ]
    }],
    "viewModel": [{
        "source": "riskTable",
        "columns": [
            {
                "title": "Type of threat/risk",
                "source": "riskType",
                "type": "selectOne"
            },
            {
                "title": "Description",
                "source": "riskDescription",
                "type": "textarea"
            },
            {
                "title": "Likelihood",
                "source": "riskLikelihood",
                "type": "selectOne"
            },
            {
                "title": "Consequence",
                "source": "riskConsequence",
                "type": "selectOne"
            },
            {
                "title": "Risk rating",
                "source": "riskRating",
                "computed": null,
                "type": "selectOne"
            }
        ],
        "userAddedRows": true,
        "type": "table"
    }]
}