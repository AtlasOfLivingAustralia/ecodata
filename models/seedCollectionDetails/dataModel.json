{
    "modelName": "Seed Collection Details",
    "dataModel": [
        {
            "dataType": "list",
            "name": "seedsList",
            "columns": [
                {
                    "dataType": "species",
                    "name": "seedSourceSpecies"
                },
                {
                    "dataType": "number",
                    "name": "seedCollectedKg"
                },
                {
                    "dataType": "text",
                    "name": "sourcePlantHealth",
                    "constraints": [
                        "Healthy young plant",
                        "Healthy mature plant",
                        "Healthy senescent plant",
                        "Environmentally stressed young plant",
                        "Environmentally stressed mature plant",
                        "Environmentally stressed senescent plant"
                    ]
                }
            ]
        },
        {
            "dataType": "text",
            "name": "seedSourceProvenance"
        },
        {
            "dataType": "text",
            "name": "seedBankRepository"
        },
        {
            "dataType": "text",
            "name": "notes"
        }
    ],
    "viewModel": [{
        "items": [
            {
                "source": "seedSourceProvenance",
                "preLabel": "Provenance of source plant(s)",
                "type": "text",
                "validate": "required"
            },
            {
                "source": "seedBankRepository",
                "preLabel": "Name of seed bank storing the seed",
                "type": "text"
            },
            {
                "source": "seedsList",
                "columns": [
                    {
                        "title": "Species",
                        "source": "seedSourceSpecies",
                        "type": "autocomplete",
                        "validate": "required"
                    },
                    {
                        "title": "Seed collected (Kg)",
                        "source": "seedCollectedKg",
                        "type": "number",
                        "validate": "required"
                    },
                    {
                        "title": "Health of source plant(s)",
                        "source": "sourcePlantHealth",
                        "type": "selectOne"
                    }
                ],
                "userAddedRows": "true",
                "type": "table",
                "printRows" : 6
            },
            {
                "source": "notes",
                "preLabel": "Comments / Notes",
                "width": "60%",
                "type": "textarea"
            }
        ],
        "class": "output-section",
        "type": "row"
    }]
}