{
    "modelName": "Weed Observation / Monitoring Details",
    "dataModel": [
        {
            "dataType": "list",
            "name": "weedObservationMonitoringDetails",
            "columns": [
                {
                    "dataType": "species",
                    "name": "weedSpecies"
                },
                {
                    "dataType": "number",
                    "name": "weedCoverHa"
                },
                {
                    "dataType": "text",
                    "name": "weedInfestationDensity",
                    "constraints": [
                        "Dense general cover",
                        "Dense patches",
                        "Few scattered plants",
                        "Moderately dense general cover",
                        "Moderately dense scattered patches",
                        "Sparse scattered patches",
                        "Sparse general cover"
                    ]
                },
                {
                    "dataType": "text",
                    "name": "weedHealth",
                    "constraints": [
                        "Dead",
                        "Healthy",
                        "Stressed"
                    ]
                },
                {
                    "dataType": "text",
                    "name": "weedReproductiveStatus",
                    "constraints": [
                        "Flowering (FLWG)",
                        "Immature (IMTR)",
                        "Not recorded (NRRS)",
                        "Seeding (SEDG)",
                        "Vegetative & Mature (VGTV)"
                    ]
                },
                {
                    "dataType": "text",
                    "name": "weedStatus",
                    "constraints": [
                        "Active Infestation",
                        "Closed",
                        "New Infestation",
                        "Under control",
                        "Under monitoring",
                        "Unknown"
                    ]
                }
            ],
            "type": "row",
            "class": "output-section"
        },
        {
            "items": [
                {
                    "dataType": "text",
                    "name": "weedInspectionNotes"
                }
            ],
            "type": "row",
            "class": "output-section"
        }
    ],
    "viewModel": [
        {
            "source": "weedObservationMonitoringDetails",
            "columns": [
                {
                    "title": "Weed species *",
                    "source": "weedSpecies",
                    "type": "autocomplete",
                    "validate": "required"
                },
                {
                    "title": "Weed cover (Ha) *",
                    "source": "weedCoverHa",
                    "type": "number",
                    "validate": "required"
                },
                {
                    "title": "Infestation Density",
                    "source": "weedInfestationDensity",
                    "type": "selectOne"
                },
                {
                    "title": "Health",
                    "source": "weedHealth",
                    "type": "selectOne"
                },
                {
                    "title": "Reproductive status",
                    "source": "weedReproductiveStatus",
                    "type": "selectOne"
                },
                {
                    "title": "Weed Status",
                    "source": "weedStatus",
                    "type": "selectOne"
                }
            ],
            "userAddedRows": "true",
            "type": "table",
            "class": "output-section"
        },
        {
            "items": [
                {
                    "preLabel": "Notes",
                    "source": "weedInspectionNotes",
                    "type": "textarea"
                }
            ],
            "type": "row",
            "class": "output-section"
        }
    ]
}