            "dataType": "stringList",
            "description": "The type of management action(s) implemented to facilitate the practice change",
            "name": "managementAction",
            "constraints": [
                "Reduce intensity of existing landuse",
                "Implement conservation farming measures",
                "Apply improved technologies in management",
                "Reduce adverse off-farm impacts of current practice",
                "Fence out / manage stock access to watering points",
                "Change landuse / industry",
                "Other (specify in notes)"
            ]
        },
        {
            "dataType": "number",
            "description": "Area of land in hectares over which environmental benefits are attained from the changed management practices",
            "name": "benefitAreaHa"
        },
        {
            "dataType": "stringList",
            "description": "Targeted outcomes to be attained from the changed management practices",
            "name": "targetOutcomes",
            "constraints": [
                "Reduced transport of nutrients off-farm",
                "Reduced transport of soils / sediment off-farm",
                "Reduced transport of pesticides off-farm",
                "Reduced transport of farm chemicals off-farm",
                "Improved soil structure, health and fertility / productivity",
                "Enhanced habitat for native flora and fauna",
                "Other (specify in notes)"
            ]
        },
        {
            "dataType": "number",
            "description": "Length of fence in kilometres erected to control stock access to watering points",
            "name": "lengthOfFenceStockWatering"
        },
        {
            "dataType": "textarea",
            "name": "notes"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "description": "Aggregate total of the area in hectares on which land management practice changes were implemented",
            "name": "totalChangePracticeTreatedArea",
            "computed": {
                "operation": "sum",
                "dependents": {
                    "source": "changePracticeTreatedArea",
                    "fromList": "practiceChange"
                }
            }
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "description": "Aggregate total of the number of farming entities adopting land management practice changes",
            "name": "totalEntitiesAdoptingChange",
            "computed": {
                "operation": "sum",
                "dependents": {
                    "source": "entitiesAdoptingChange",
                    "fromList": "practiceChange"
                }
            }
        }
