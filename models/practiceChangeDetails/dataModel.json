{
    "modelName": "Land Management Practice Change Details",
    "dataModel": [
        {
            "dataType": "list",
            "name": "practiceChange",
            "columns": [
                {
                    "dataType": "text",
                    "description": "The purpose for which changed practices are implemented",
                    "name": "changePurpose",
                    "constraints": [
                        "Sustainable farm practice",
                        "Sustainable fishing and aquaculture practice",
                        "Innovative farming and fishing practices",
                        "Reduce run-off into the Great Barrier Reef",
                        "Wetland, riparian and mangrove protection and restoration along the Great Barrier Reef",
                        "Other (specify in notes)"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "description": "The type of industry in which changed practices are implemented",
                    "name": "industryType",
                    "constraints": [
                        "Sugarcane",
                        "Grazing",
                        "Grains",
                        "Grapes",
                        "Vegetables",
                        "Fruit trees",
                        "Berries",
                        "Dairy",
                        "Cotton",
                        "Forestry",
                        "Aquaculture",
                        "Fishing",
                        "Other (specify in notes)"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "description": "The condition of the resource at the time when changed practices are implemented",
                    "name": "resourceCondition",
                    "constraints": [
                        "Lack of ground cover",
                        "Soil acidity",
                        "Soil erosion (wind)",
                        "Soil erosion (water)",
                        "Soil health (biology)",
                        "Soil health (other)",
                        "Quality of wastewater (aquaculture)",
                        "Marine quality (fishing)",
                        "Nutrient loads",
                        "Sediment loads",
                        "Pesticide loads",
                        "Vegetation condition",
                        "Other (specify in notes)"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "description": "The practice change action applied to address the resource condition issue.",
                    "name": "practiceChangeAction",
                    "constraints": [
                        "Retaining ground cover",
                        "Soil testing",
                        "Minimum-to-no till farming",
                        "Composting/mulching",
                        "Planting cover crops",
                        "Farm planning",
                        "Soil conditioning",
                        "Managing soil nutrients",
                        "Establishing wind breaks",
                        "Planning",
                        "By-catch reduction",
                        "Industry environmental management plan",
                        "Industry codes of practice",
                        "Reduced stocking rates/selective grazing",
                        "Fence off stock to watering points",
                        "Consistent wheel tracks",
                        "Crop rotation improvements",
                        "Nitrogen budget developed",
                        "Chemical application considered with rain forecast",
                        "Revegetation",
                        "Restoration",
                        "Riparian fencing",
                        "Management of invasive species",
                        "Improved technologies in management",
                        "Other (specify in notes)"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "number",
                    "description": "Area of land in hectares on which changed management practices are implemented",
                    "name": "changePracticeTreatedArea",
                    "validate": "required"
                },
                {
                    "dataType": "number",
                    "description": "The number of farming entities adopting the specified changed management practices",
                    "name": "entitiesAdoptingChange",
                    "validate": "required"
                }
            ]
        },
        {
            "dataType": "number",
            "description": "Area of land in hectares over which environmental benefits are attained from the changed management practices",
            "name": "benefitAreaHa",
            "validate": "required"
        },
        {
            "dataType": "stringList",
            "description": "Targeted outcomes to be attained from the changed management practices",
            "name": "targetOutcomes",
            "constraints": [
                "Reduced transport of nutrients off-farm",
                "Reduced transport of soils / sediment off-farm",
                "Reduced transport of pesticides off-farm",
                "Reduced transport of farm chemicals off-farm",
                "Improved soil structure, health and fertility / productivity",
                "Enhanced habitat for native flora and fauna",
                "Other (specify in notes)"
            ]
        },
        {
            "dataType": "number",
            "description": "Length of fence in kilometres erected to control stock access to watering points",
            "name": "lengthOfFenceStockWatering"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "description": "Aggregate total of the area in hectares on which land management practice changes were implemented",
            "name": "totalChangePracticeTreatedArea",
            "computed": {
                "operation": "sum",
                "dependents": {
                    "source": "changePracticeTreatedArea",
                    "fromList": "practiceChange"
                }
            }
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "description": "Aggregate total of the number of farming entities adopting land management practice changes",
            "name": "totalEntitiesAdoptingChange",
            "computed": {
                "operation": "sum",
                "dependents": {
                    "source": "entitiesAdoptingChange",
                    "fromList": "practiceChange"
                }
            }
        },
        {
            "dataType": "text",
            "name": "notes"
        }
    ],
    "viewModel": [
        {
            "footer": {"rows": [{"columns": [
                {
                    "colspan": 2,
                    "source": "",
                    "computed": null,
                    "type": "literal"
                },
                {
                    "source": "Total area of adopted changed practices (Ha)",
                    "computed": null,
                    "type": "literal"
                },
                {
                    "source": "totalChangePracticeTreatedArea",
                    "computed": "{\"operation\":\"sum\",\"dependents\":{\"source\":\"changePracticeTreatedArea\",\"fromList\":\"practiceChange\"}}",
                    "type": "number"
                },
                {
                    "source": "Total No. of farming entities",
                    "computed": null,
                    "type": "literal"
                },
                {
                    "source": "totalEntitiesAdoptingChange",
                    "computed": "{\"operation\":\"sum\",\"dependents\":{\"source\":\"entitiesAdoptingChange\",\"fromList\":\"practiceChange\"}}",
                    "type": "number"
                }
            ]}]},
            "title": "Please list the details for each type of land management practice change related to this activity and any additional information that you can that is relevant:",
            "source": "practiceChange",
            "columns": [
                {
                    "title": "Industry From:",
                    "source": "changePurpose",
                    "computed": null,
                    "width": "25%",
                    "type": "selectOne"
                },
                {
                    "title": "Industry:",
                    "source": "industryType",
                    "computed": null,
                    "type": "selectOne"
                },
                {
                    "title": "Resource condition prior to action:",
                    "source": "resourceCondition",
                    "computed": null,
                    "type": "selectOne"
                },
                {
                    "title": "Practice change action:",
                    "source": "practiceChangeAction",
                    "computed": null,
                    "width": "20%",
                    "type": "selectOne"
                },
                {
                    "title": "Area treated by practice change (Ha):",
                    "source": "changePracticeTreatedArea",
                    "computed": null,
                    "width": "10%",
                    "type": "number"
                },
                {
                    "title": "No. of farming entities adopting this practice change:",
                    "source": "entitiesAdoptingChange",
                    "computed": null,
                    "width": "10%",
                    "type": "number"
                }
            ],
            "class": "output-section",
            "userAddedRows": true,
            "editableRows": "true",
            "type": "table"
        },
        {
            "items": [
                {
                    "items": [
                        {
                            "source": "benefitAreaHa",
                            "computed": null,
                            "preLabel": "Area of land directly benefiting from the practice change:",
                            "type": "number"
                        },
                        {
                            "source": "lengthOfFenceStockWatering",
                            "computed": null,
                            "preLabel": "Length of fence installed for managing stock access to water:",
                            "type": "number"
                        }
                    ],
                    "computed": null,
                    "type": "col"
                },
                {
                    "items": [{
                        "source": "targetOutcomes",
                        "computed": null,
                        "preLabel": "Expected outcomes from changed practices:",
                        "type": "selectMany"
                    }],
                    "computed": null,
                    "type": "col"
                }
            ],
            "class": "output-section",
            "type": "row"
        },
        {
            "items": [{
                "source": "notes",
                "computed": null,
                "preLabel": "Comments / Notes",
                "width": "90%",
                "type": "textarea"
            }],
            "class": "output-section",
            "type": "row"
        }
    ]
}