{
    "modelName":"Indigenous Employment Details",
    "dataModel":[
        {"name":"indigenousEmployment", "dataType":"list","columns":[
            {"name":"category","dataType":"text"},
            {"name":"numFullTime","dataType":"number"},
            {"name":"numPartTime","dataType":"number"},
            {"name":"numMale","dataType":"number"},
            {"name":"numFemale","dataType":"number"},
            {"name":"totalIndividuals","dataType":"number","computed":{"dependents":["numFullTime","numPartTime"],"operation":"sum"}}
        ],
        "defaultRows":[
            {"category":"Indigenous Rangers"},
            {"category":"Indigenous Employees (Non-rangers)"},
        ]
        },

        {"name":"totalFtEmployees","dataType":"number","primaryResult":"true",
            "computed":{
                "dependents":{
                    "fromList":"indigenousEmployment",
                    "source":"numFullTime"
                },"operation":"sum"
            }
        },

        {"name":"totalPtEmployees","dataType":"number","primaryResult":"true",
            "computed":{
                "dependents":{
                    "fromList":"indigenousEmployment",
                    "source":"numPartTime"
                },"operation":"sum"
            }
        },

        {"name":"totalMaleEmployees","dataType":"number","primaryResult":"true",
            "computed":{
                "dependents":{
                    "fromList":"indigenousEmployment",
                    "source":"numMale"
                },"operation":"sum"
            }
        },

        {"name":"totalFemaleEmployees","dataType":"number","primaryResult":"true",
            "computed":{
                "dependents":{
                    "fromList":"indigenousEmployment",
                    "source":"numFemale"
                },"operation":"sum"
            }
        },

        {"name":"totalEmployees","dataType":"number","primaryResult":"true",
            "computed":{
                "dependents":{
                    "fromList":"indigenousEmployment",
                    "source":"totalIndividuals"
                },"operation":"sum"
            }
        }


     ],
    "viewModel":[
        {   "type": "table", "source":"indigenousEmployment",
            "columns": [
                {"title":"Employment Category", "source":"category","type":"text", "noEdit":true},
                {"title":"Full Time","source":"numFullTime","type":"number","validate":"min[0]"},
                {"title":"Part Time","source":"numPartTime","type":"number","validate":"min[0]"},
                {"title":"Male","source":"numMale","type":"number","validate":"min[0]"},
                {"title":"Female","source":"numFemale","type":"number","validate":"min[0]"},
                {"title":"Total Individuals","source":"totalIndividuals","type":"number", "noEdit":true}
            ],
            "footer": {
                "rows": [
                    {"columns":[
                        {"type":"literal","source":"Total"},
                        {"type":"number","source":"totalFtEmployees"},
                        {"type":"number","source":"totalPtEmployees"},
                        {"type":"number","source":"totalMaleEmployees"},
                        {"type":"number","source":"totalFemaleEmployees"},
                        {"type":"number","source":"totalEmployees"}
                    ]}
                ]
            }
        }
    ]
}




