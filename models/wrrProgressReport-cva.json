      "template": {
        "pre-populate": [
          {
            "mapping": [
              {
                "mapping": [
                  {
                    "source-path": "storyDetails",
                    "target": "storyDetails"
                  }
                ],
                "source-path": "goodNewsStories",
                "target": "goodNewsStories"
              }
            ],
            "merge": false,
            "source": {
              "literal": {
                "goodNewsStories": [
                  {
                    "storyDetails": ""
                  },
                  {
                    "storyDetails": ""
                  }
                ]
              }
            }
          },
          {
            "mapping": [
              {
                "mapping": [],
                "source-path": "issue",
                "target": "issue"
              }
            ],
            "merge": false,
            "source": {
              "literal": {
                "issue": []
              }
            }
          }
        ],
        "dataModel": [
          {
            "dataType": "text",
            "name": "organisationDetailsChanged",
            "constraints": [
              "Yes",
              "No"
            ],
            "validate": "required"
          },
          {
            "dataType": "text",
            "name": "detailsOfOrganisationChange",
            "validate": "required,maxSize[1000]",
            "behaviour": [
              {
                "type": "enable",
                "condition": "organisationDetailsChanged == \"Yes\""
              }
            ]
          },
          {
            "dataType": "text",
            "name": "projectContactDetailsChanged",
            "constraints": [
              "Yes",
              "No"
            ],
            "validate": "required"
          },
          {
            "dataType": "text",
            "name": "detailsOfProjectContacts",
            "validate": "required,maxSize[1000]",
