            "description": "Has reportable WHS incidents",
            "constraints": [
              "Yes",
              "No"
            ]
          },
          {
            "dataType": "text",
            "name": "whsIncidentDetails",
            "description": "Please provide a description of the incident/s",
            "behaviour": [
              {
                "type": "enable",
                "condition": "hasReportableIncidents == \"Yes\""
              }
            ],
            "validate": "required,maxSize[1000]"
          },
          {
            "dataType": "list",
            "description": "",
