{
    "title": "Native Herbivore Management",
    "modelName": "ESP Native Herbivore Management",
    "dataModel": [
        {
            "dataType": "list",
            "name": "nativeSpeciesMonitoring",
            "columns": [
                {
                    "dataType": "text",
                    "description": "",
                    "name": "species",
                    "constraints": [
                        "Grey Kangaroo",
                        "Red Kangaroo",
                        "Wallaby"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "season",
                    "constraints": [
                        "Spring",
                        "Summer",
                        "Autumn",
                        "Winter"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "stringList",
                    "name": "managementActions",
                    "constraints": [
                        "Exclusion fencing",
                        "Observed",
                        "Shooting"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "managementReason",
                    "constraints": [
                        "Excessive grazing",
                        "Excessive population",
                        "Land degradation",
                        "Nil"
                    ]
                },
                {
                    "dataType": "number",
                    "name": "noCulled",
                    "validate": "min[0]"
                },
                {
                    "dataType": "number",
                    "name": "cost",
                    "validate": "integer,min[0]"
                }
            ]
        }
    ],
    "viewModel": [
        {
            "items": [
                {
                    "source": "<i>Please note that this includes the following management actions: “Monitor and manage total grazing pressure”, “Monitor and manage grazing pressure from native species” and “Monitor and manage native herbivores”.</i>",
                    "type": "literal"
                }
            ],
            "type": "row"
        },
        {
            "items": [
                {
                    "title": "Please complete the following:",
                    "source": "nativeSpeciesMonitoring",
                    "columns": [
                        {
                            "title": "Species",
                            "source": "species",
                            "type": "selectOne"
                        },
                        {
                            "title": "Season observed / managed",
                            "source": "season",
                            "type": "selectOne"
                        },
                        {
                            "title": "Management Action(s)",
                            "source": "managementActions",
                            "type": "selectOne"
                        },
                        {
                            "title": "Reason for management",
                            "source": "managementReason",
                            "type": "selectOne"
                        },
                        {
                            "title": "If applicable, no. culled (if known)",
                            "source": "noCulled",
                            "type": "number"
                        },
                        {
                            "title": "Cost",
                            "source": "cost",
                            "type": "currency"
                        }
                    ],
                    "userAddedRows": true,
                    "disableTableUpload":true,
                    "type": "table"
