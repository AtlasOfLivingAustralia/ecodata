{
    "title": "Native Herbivore Management",
    "modelName": "ESP Native Herbivore Management",
    "dataModel": [
        {
            "dataType": "list",
            "name": "nativeSpeciesMonitoring",
            "columns": [
                {
                    "dataType": "text",
                    "description": "",
                    "name": "species",
                    "constraints": [
                        "Grey Kangaroo",
                        "Red Kangaroo",
                        "Wallaby"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "season",
                    "constraints": [
                        "Spring",
                        "Summer",
                        "Autumn",
                        "Winter"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "stringList",
                    "name": "managementActions",
                    "constraints": [
                        "Exclusion fencing",
                        "Observed",
                        "Shooting"
                    ],
                    "validate": "required"
                },
                {
                    "dataType": "text",
                    "name": "managementReason",
                    "constraints": [
                        "Excessive grazing",
                        "Excessive population",
                        "Land degradation",
                        "Nil"
                    ]
                },
                {
                    "dataType": "number",
                    "name": "noCulled",
                    "validate": "min[0]"
                }
            ]
        },
        {
            "dataType": "number",
            "name": "cost",
            "validate": "integer,min[0]"
        },
        {
            "dataType":"text",
            "name":"managementActionNotCompleted",
            "description":"Was this management action completed this year.",
            "constraints":[
                "Yes",
                "No"
            ],
            "validate":"required"
        },
        {
            "dataType": "text",
            "name": "notes",
            "description": ""
        }
    ],
    "viewModel": [
        {
            "items": [
                {
                    "source": "<i>Please note that this includes the following management actions: “Monitor and manage total grazing pressure”, “Monitor and manage grazing pressure from native species” and “Monitor and manage native herbivores”.</i>",
                    "type": "literal"
                }
            ],
            "type": "row"
        },
        {
            "items": [
                {
                    "title": "Please complete the following:",
                    "source": "nativeSpeciesMonitoring",
                    "columns": [
                        {
                            "title": "Species",
                            "source": "species",
                            "type": "selectOne"
                        },
                        {
                            "title": "Season observed / managed",
                            "source": "season",
                            "type": "selectOne"
                        },
                        {
                            "title": "Management Action(s)",
                            "source": "managementActions",
                            "type": "select2Many"
                        },
                        {
                            "title": "Reason for management",
                            "source": "managementReason",
                            "type": "selectOne"
                        },
                        {
                            "title": "If applicable, no. culled (if known)",
                            "source": "noCulled",
                            "type": "number"
                        }
                    ],
                    "userAddedRows": true,
                    "disableTableUpload":true,
                    "type": "table"
                }
            ],
            "type": "row"
        },
        {
            "type":"row",
            "items":[
                {
                    "preLabel": "Total cost of management actions",
                    "source": "cost",
                    "type": "currency"
                }
            ]
        },
        {
            "type":"row",
            "items":[
                {
                    "type":"col",
                    "items":[
                        {
                            "type":"selectOne",
                            "source":"managementActionNotCompleted",
                            "preLabel":"Have you chosen to not undertake activities under this Management action this year? (Yes / No)"
                        }
                    ]
                }

            ]
        },
        {
            "type": "row",
            "items": [
                {
                    "type": "textarea",
                    "source": "notes",
                    "preLabel": "If you have chosen to not undertake activities under this Management action or would like to provide further observations or comments regarding activities you have undertaken please fill out the textbox below."
                }
            ]
        }
    ]
}