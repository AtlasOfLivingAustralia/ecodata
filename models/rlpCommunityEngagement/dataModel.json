{
  "title": "Community/stakeholder engagement",
  "modelName": "RLP - Community engagement",
  "dataModel": [
    {
      "dataType":"list",
      "name":"events",
      "columns":[
        {
          "dataType": "text",
          "name": "eventType",
          "constraints":[
            "Field day",
            "Training / workshop event",
            "Conference / seminar",
            "One-on-one technical advice interaction",
            "On-ground trial / demonstration",
            "On-ground works"
          ],
          "validate": "required"
        },
        {
          "dataType": "number",
          "name": "numberOfParticipants",
          "validate": "required,min[0]"
        },
        {
          "dataType": "number",
          "name": "numberOfIndigenousParticipants",
          "validate": "required,min[0]"
        },
        {
          "dataType": "number",
          "name": "numberOfFarmers",
          "validate": "required,min[0]"
        },
        {
          "dataType": "stringList",
          "name": "industryType",
          "constraints":[
            "n/a",
            "Broad acre cropping",
            "Dairy",
            "Horticulture",
            "Grazing",
            "Fisheries",
            "Aquaculture"
          ],
          "validate": "required,min[0]"
        },
        {
          "dataType": "text",
          "name": "eventPurpose",
          "validate": "required,max[80]"
        }
      ]
    }

  ],
  "viewModel": [

    {
      "items": [
        {
          "columns": [
            {
              "source": "eventType",
              "title": "Community engagement activity type",
              "type": "select2",
              "width":"20%"
            },
            {
              "source": "eventPurpose",
              "title": "Aim of activity",
              "type": "textarea",
              "width":"30%"
            },
            {
              "source": "numberOfParticipants",
              "title": "Total number of attendees / participants",
              "type": "number",
              "width":"10%"
            },
            {
              "source": "numberOfParticipants",
              "title": "Total number of Indigenous attendees / participants",
              "type": "number",
              "width":"10%"
            },
            {
              "source": "numberOfFarmers",
              "title": "Total number of farmers attending / participating",
              "type": "number",
              "width":"10%"
            },
            {
              "source": "industryType",
              "title": "Industry type engaged",
              "type": "select2Many",
              "width":"20%"
            }

          ],
          "source":"events",
          "userAddedRows":true,
          "type": "table"
        }
      ],
      "type": "section"
    }
  ]
}