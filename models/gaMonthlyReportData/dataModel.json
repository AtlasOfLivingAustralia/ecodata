                        {
                            "source": "trainingCommencedAccreditedTotal",
                            "computed": "{\"operation\":\"sum\",\"dependents\":{\"source\":[\"trainingCommencedAccreditedNonIndigenous\",\"trainingCommencedAccreditedIndigenous\"]}}",
                            "preLabel": "Commenced accredited training:",
                            "noEdit": "true",
