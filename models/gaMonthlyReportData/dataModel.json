{
    "modelName": "Monthly Status Report Data",
    "dataModel": [
        {
            "dataType": "text",
            "description": "Indicate the most appropriate status of the project at the end of the reporting month.",
            "name": "projectStatus",
            "constraints": [
                "Not started",
                "Agreement signed",
                "Agreement signed and project commenced",
                "Commenced",
                "Progressing - behind schedule",
                "Progressing - on schedule",
                "Progressing - ahead of schedule",
                "Completed",
                "Abandoned"
            ],
            "validate": "required"
        },
        {
            "dataType": "date",
            "description": "If the project is running behind schedule you are required to explain why.",
            "name": "projectStatusReason"
        },
        {
            "dataType": "text",
            "description": "If the project is running behind schedule, please explain how you intend to bring it back on schedule, including what actions you plan to take.",
            "name": "remedialActions"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who entered into a Training Plan for the Project in the current month that included training accredited under the Australian Qualifications Framework.",
            "name": "trainingCommencedAccreditedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who entered into a Training Plan for the Project in the current month that included training accredited under the Australian Qualifications Framework.",
            "name": "trainingCommencedAccreditedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who entered into Training Plans for the Project in the current month that did not include training accredited under the Australian Qualifications Framework. These Participants are therefore undertaking the minimum level of training required for participation in the Project.",
            "name": "trainingCommencedNonaccreditedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who entered into Training Plans for the Project in the current month that did not include training accredited under the Australian Qualifications Framework. These Participants are therefore undertaking the minimum level of training required for participation in the Project.",
            "name": "trainingCommencedNonaccreditedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who did not fulfil the requirements of their Training Plan and therefore did not complete (failed) their required training under the Project in the current month. This might include Participants who exited the Project early, opted out of training for the remainder of the Project, or were unable to satisfy their training requirements.",
            "name": "trainingNoExitedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who did not fulfil the requirements of their Training Plan and therefore did not complete (failed) their required training under the Project in the current month. This might include Participants who exited the Project early, opted out of training for the remainder of the Project, or were unable to satisfy their training requirements.",
            "name": "trainingNoExitedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who fulfilled the requirements of their Training Plan and therefore completed (passed) their required training under the Project in the current month. Includes both accredited and non-accredited training.",
            "name": "trainingNoCompletedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who fulfilled the requirements of their Training Plan and therefore completed (passed) their required training under the Project in the current month. Includes both accredited and non-accredited training.",
            "name": "trainingNoCompletedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who entered into a Participant Agreement for the Project in the current month.",
            "name": "totalParticipantsCommencedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who entered into a Participant Agreement for the Project in the current month.",
            "name": "totalParticipantsCommencedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who did not fulfil the requirements of their Participant Agreement and therefore did not complete (failed) the Project in the current month. This number should include Participants who withdrew from the Project voluntarily (Participant initiated) and those who were exited (Service Provider initiated).",
            "name": "totalParticipantsNotCompletedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who did not fulfil the requirements of their Participant Agreement and therefore did not complete (failed) the Project in the current month. This number should include Participants who withdrew from the Project voluntarily (Participant initiated) and those who were exited (Service Provider initiated).",
            "name": "totalParticipantsNotCompletedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of non-Indigenous Participants who fulfilled the requirements of their Participant Agreement and therefore completed (passed) the Project in the current month.",
            "name": "totalParticipantsCompletedNonIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "Record the number of Indigenous Participants who fulfilled the requirements of their Participant Agreement and therefore completed (passed) the Project in the current month.",
            "name": "totalParticipantsCompletedIndigenous",
            "validate": "required,min[0]"
        },
        {
            "dataType": "number",
            "description": "We are interested in knowing how long it takes you to complete your reporting requirements. When submitting your reports to the Green Army Team, please tell us the time (in minutes) you have taken to complete this report.",
            "name": "reportingTimeTaken",
            "validate": "required, min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "trainingCommencedAccreditedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "trainingCommencedAccreditedNonIndigenous",
                    "trainingCommencedAccreditedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "trainingCommencedNonaccreditedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "trainingCommencedNonaccreditedNonIndigenous",
                    "trainingCommencedNonaccreditedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "trainingNoExitedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "trainingNoExitedNonIndigenous",
                    "trainingNoExitedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "trainingNoCompletedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "trainingNoCompletedNonIndigenous",
                    "trainingNoCompletedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "totalParticipantsCommencedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "totalParticipantsCommencedNonIndigenous",
                    "totalParticipantsCommencedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "totalParticipantsNotCompletedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "totalParticipantsNotCompletedNonIndigenous",
                    "totalParticipantsNotCompletedIndigenous"
                ]}
            },
            "validate": "min[0]"
        },
        {
            "dataType": "number",
            "primaryResult": "true",
            "name": "totalParticipantsCompletedTotal",
            "computed": {
                "operation": "sum",
                "dependents": {"source": [
                    "totalParticipantsCompletedNonIndigenous",
                    "totalParticipantsCompletedIndigenous"
                ]}
            },
            "validate": "min[0]"
        }
    ],
    "viewModel": [
        {
            "items": [{
                "source": "Please indicate the current status of the project at the time of this report. Note that it is mandatory to provide a reason and the remedial actions to be implemented if the project is tracking behind schedule.",
                "computed": null,
                "type": "literal"
            }],
            "type": "row"
        },
        {
            "items": [
                {
                    "items": [{
                        "source": "projectStatus",
                        "computed": null,
                        "preLabel": "How is the project tracking?:",
                        "type": "selectOne"
                    }],
                    "computed": null,
                    "width": "30%",
                    "type": "col"
                },
                {
                    "items": [{
                        "source": "projectStatusReason",
                        "computed": null,
                        "preLabel": "Reason project is behind schedule or that MERIT has not been updated:",
                        "type": "textarea"
                    }],
                    "computed": null,
                    "width": "30%",
                    "type": "col"
                },
                {
                    "items": [{
                        "source": "remedialActions",
                        "computed": null,
                        "preLabel": "Intended remedial actions (if appropriate):",
                        "type": "textarea"
                    }],
                    "computed": null,
                    "width": "30%",
                    "type": "col"
                }
            ],
            "class": "output-section",
            "type": "row"
        },
        {
